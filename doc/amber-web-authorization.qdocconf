project         = amber-web-authorization
description     = Amber Web Authorization Framework Reference Documentation
versionsym      =
version         = 1.0.0
url             = $BASE_URL/amber-web-authorization

sourcedirs += $$PWD/../lib $$PWD/../import $$PWD/../doc
headerdirs += $$PWD/../lib $$PWD/../import
exampledirs += $$PWD/../examples

outputformats = HTML
outputdir = $$PWD/../doc/html
base = file:$$PWD/../doc/html
tagfile = $$PWD/../doc/html/amber-web-authorization.tags

outputprefixes = QML
outputprefixes.QML = qml-amber-web-authorization-

qhp.projects = AmberWebAuthorization

qhp.AmberWebAuthorization.file = amber-web-authorization.qhp
qhp.AmberWebAuthorization.namespace = org.sailfishos.amber.web.authorization.1.0.0
qhp.AmberWebAuthorization.virtualFolder = amber.web.authorization
qhp.AmberWebAuthorization.indexTitle = Amber Web Authorization Framework
qhp.AmberWebAuthorization.indexRoot =

qhp.AmberWebAuthorization.filterAttributes = amber-web-authorization 1.0.0
qhp.AmberWebAuthorization.customFilters.AmberWebAuthorization.name = Amber Web Authorization 1.0.0
qhp.AmberWebAuthorization.customFilters.AmberWebAuthorization.filterAttributes = amber-web-authorization 1.0.0

qhp.AmberWebAuthorization.subprojects = overview qmltypes classes

qhp.AmberWebAuthorization.subprojects.overview.title = Amber Web Authorization Framework Overview
qhp.AmberWebAuthorization.subprojects.overview.indexTitle = Amber Web Authorization Framework Overview
qhp.AmberWebAuthorization.subprojects.overview.type = manual
qhp.AmberWebAuthorization.subprojects.qmltypes.title = QML Types
