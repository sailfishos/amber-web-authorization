qhp.AmberWebAuthorization.subprojects.qmltypes.selectors = qmlclass
qhp.AmberWebAuthorization.subprojects.qmltypes.sortPages = true
qhp.AmberWebAuthorization.subprojects.classes.title = C++ Classes
